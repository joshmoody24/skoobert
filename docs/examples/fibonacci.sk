// Fibonacci sequence using recursion
// Note: Due to lazy evaluation, large values are efficient

let fib = n =>
  n <= 1
    ? n
    : fib(n - 1) + fib(n - 2);

console.log("Fibonacci sequence:");
console.log(fib(0));  // 0
console.log(fib(1));  // 1
console.log(fib(2));  // 1
console.log(fib(3));  // 2
console.log(fib(4));  // 3
console.log(fib(5));  // 5
console.log(fib(6));  // 8
console.log(fib(7));  // 13
console.log(fib(8));  // 21
console.log(fib(9));  // 34
console.log(fib(10)); // 55